{
  "name": "accounting-api",
  "description": "Accounting API",
  "license": "ISC",
  "version": "1.0.0",
  "type": "module",
  "author": "I Made Budi Dharma Wipraja <dharmawipraja@gmail.com>",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=production node server.js",
    "dev": "NODE_ENV=development nodemon server.js",
    "prod": "NODE_ENV=production pm2 start ecosystem.config.json",
    "stop": "pm2 stop accounting-api",
    "restart": "pm2 restart accounting-api",
    "logs": "pm2 logs accounting-api",
    "monitor": "pm2 monit",
    "prisma:generate": "prisma generate --no-engine",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "node prisma/seed.js",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:lint": "npm run format && npm run lint:fix",
    "docs:openapi": "node ./scripts/generate-openapi.mjs"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "@fastify/caching": "^9.0.0",
    "@fastify/compress": "^8.1.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^10.0.0",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@fastify/under-pressure": "^9.0.0",
    "@prisma/client": "^6.13.0",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "decimal.js": "^10.5.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.5.0",
    "fastify-request-id": "^1.0.1",
    "fastify-type-provider-zod": "^5.0.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "prisma-extension-soft-delete": "^2.0.1",
    "ulid": "^3.0.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
